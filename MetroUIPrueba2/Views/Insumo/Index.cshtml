@{
    ViewBag.Title = "Intranet";
    Layout = "~/Views/Shared/_IntranetLayout.cshtml";
}

<style>
    #principal {
        width: 900px;
        height: auto;
        top: 50%;
        margin-top: 60px;
        border: 1px solid green;
        padding: 50px;
        position: relative;
    }
</style>

<div class="container" id="principal">
    <h3>Lista de Insumo</h3>
    <a href="/Insumo/nuevo" class="button primary small">Nuevo</a>
    <hr />
    <table class="table compact">
        <thead>
            <tr>
                <th>#</th>
                <th>Descripcion</th>
                <th>Unidad Medida</th>
                <th>Categoria</th>
                <th>Acciones</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>1</td>
                <td>Manillas de Bronce</td>
                <td>Unidad</td>
                <td>Accesorios</td>
                <td>
                    <a href="@Url.Action("Ver","Insumo", new { id = 1 })">Ver/</a>
                    <a href="@Url.Action("Editar", "Insumo", new {id=1 })">Editar</a>
                </td>
            </tr>
            <tr>
                <td>2</td>
                <td>Terciado Estructural</td>
                <td>M2</td>
                <td>Materia Prima</td>
                <td>
                    <a href="@Url.Action("Ver","Insumo", new { id = 1 })">Ver/</a>
                    <a href="@Url.Action("Editar", "Insumo", new {id=1 })">Editar</a>
                </td>
            </tr>
            <tr>
                <td>3</td>
                <td>Pintura Blanca</td>
                <td>Galon</td>
                <td>Pinturas</td>
                <td>
                    <a href="@Url.Action("Ver","Insumo", new { id = 1 })">Ver/</a>
                    <a href="@Url.Action("Editar", "Insumo", new {id=1 })">Editar</a>
                </td>
            </tr>
        </tbody>
    </table>

</div>



